<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Name="Dark">
            <SolidColorBrush x:Key="ActiveBrush" Color="LightGoldenrodYellow"/>
            <SolidColorBrush x:Key="BackgroundBrush" Color="Black"/>
            <SolidColorBrush x:Key="BackgroundTransparentBrush" Color="#EA000000"/>
            <SolidColorBrush x:Key="BorderBrush" Color="#FF2F2F2F"/>
            <SolidColorBrush x:Key="ContentWindowBrush" Color="#FF232323"/>
            <SolidColorBrush x:Key="InActiveBrush" Color="Coral"/>
            <SolidColorBrush x:Key="NavbarBrush" Color="#FF111111"/>
            <SolidColorBrush x:Key="ReagentBrush" Color="#FF0B4747"/>
            <SolidColorBrush x:Key="WingPanelBrush" Color="#BF000000"/>

            <SolidColorBrush x:Key="TextControlBackgroundFocused" Color="Black"/>
            <SolidColorBrush x:Key="TextControlForegroundFocused" Color="White"/>
            <SolidColorBrush x:Key="TextControlBorderBrushFocused" Color="White"/>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
    
</ResourceDictionary>
